      "['Пол, возраст', 'Ищет работу на должность:', 'Последнее/нынешнее место работы', 'Последняя/нынешняя должность', 'Обновление резюме', 'Авто', 'Образование', 'Sex', 'Age', 'Опыт работы (месяц)', 'Город', 'Готовность к переезду', 'Готовность к командировкам', 'Занятость_полная занятость', 'Занятость_частичная занятость', 'Занятость_проектная работа', 'Занятость_волонтерство', 'Занятость_стажировка', 'График_полный день', 'График_сменный график', 'График_гибкий график', 'График_удаленная работа', 'График_вахтовый метод', 'ЗП (руб)']\n"
     ]
    }
   ],
   "source": [
    "rates = pd.read_csv('/Users/yaroslavbaev/Desktop/miphi/ExchangeRates.csv', sep = ',')\n",
    "currency_map = {\n",
    "    'руб.': 'RUB',\n",
    "    'USD': 'USD', \n",
    "    'грн.': 'UAH',\n",
    "    'KZT': 'KZT',\n",
    "    'EUR': 'EUR',\n",
    "    'GBP': 'GBP',\n",
    "    'AZN': 'AZN'\n",
    "}\n",
    "\n",
    "hh['date'] = pd.to_datetime(hh['Обновление резюме'], format='%d.%m.%Y %H:%M').dt.date\n",
    "rates_dict = {}\n",
    "for _, row in rates.iterrows():\n",
    "    try:\n",
    "        currency = row['currency']\n",
    "        date_str = row['date']  \n",
    "        date_obj = pd.to_datetime(date_str.replace('D ', ''), format='%d/%m/%y').date()\n",
    "        close_price = float(row['close'])\n",
    "        proportion = float(row['proportion'])\n",
    "        rate = close_price / proportion\n",
    "        rates_dict[(currency, date_obj)] = rate\n",
    "    except Exception as e:\n",
    "        continue\n",
    "hh['ЗП (руб)'] = hh.apply(\n",
    "    lambda row: (\n",
    "        lambda salary_str, date: (\n",
    "            np.nan if pd.isna(salary_str) else (\n",
    "                lambda m: (\n",
    "                    int(float(m.group(1))) if (curr_str := m.group(2).strip()) == 'руб.' else (\n",
    "                        int(float(m.group(1)) * rates_dict.get((currency_map.get(curr_str, curr_str), date), np.nan))\n",
    "                        if (curr_code := currency_map.get(curr_str, None)) and (rate := rates_dict.get((curr_code, date))) else np.nan\n",
    "                    )\n",
    "                ) if m else np.nan\n",
    "            )(re.match(r'(\\d+(?:[.,]\\d+)?)\\s*(\\D+)', str(salary_str).replace(',', '.').strip()))\n",
    "        )\n",
    "    )(row['ЗП'], row['date']),\n",
    "    axis=1\n",
    ")\n",
    "\n",
    "hh['ЗП (руб)'] = hh['ЗП (руб)'].astype('Int64')\n",
    "\n",
    "print(hh[['ЗП', 'Обновление резюме', 'ЗП (руб)']].head(20))\n",
    "print(hh['ЗП (руб)'].describe())\n",
    "print(f\"\\Количество пропусков/ошибок {hh['ЗП (руб)'].isna().sum()}\")\n",
    "print(f\"Всего обработано {len(hh)}\")\n",
    "hh = hh.drop(['ЗП', 'date'], axis=1)\n",
    "print(hh.columns.tolist())"
   ]
  },
