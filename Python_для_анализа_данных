   "source": [
    "import re\n",
    "\n",
    "def ex_work(text):\n",
    "    if pd.isna(text) or text == 'Не указано':\n",
    "        return np.nan\n",
    "\n",
    "    years = 0\n",
    "    months = 0\n",
    "    \n",
    "    years_match = re.search(r'(\\d+)\\s*(?:года|лет|год)', text)\n",
    "    if years_match:\n",
    "        years = int(years_match.group(1))\n",
    "    \n",
    "    months_match = re.search(r'(\\d+)\\s*месяц', text)\n",
    "    if months_match:\n",
    "        months = int(months_match.group(1))\n",
    "    \n",
    "    if years == 0 and months == 0:\n",
    "        return np.nan\n",
    "    \n",
    "    total_months = years * 12 + months\n",
    "    \n",
    "    return total_months\n",
    "\n",
    "hh['Опыт работы (месяц)'] = hh['Опыт работы'].apply(ex_work)\n",
    "print(hh['Опыт работы (месяц)'].unique()[:10])\n",
    "print(hh['Опыт работы (месяц)'].isna().sum())\n",
    "print(hh['Опыт работы (месяц)'].describe())\n",
    "print(hh[['Опыт работы', 'Опыт работы (месяц)']].head(10))\n",
    "hh = hh.drop('Опыт работы', axis=1)\n",
    "\n",
    "print(hh.columns.tolist())"
   ]
  },
