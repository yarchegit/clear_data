   "source": [
    "million_cities = ['Новосибирск', 'Екатеринбург','Нижний Новгород','Казань', \n",
    "                  'Челябинск','Омск', 'Самара', 'Ростов-на-Дону', 'Уфа', \n",
    "                  'Красноярск', 'Пермь', 'Воронеж','Волгоград']\n",
    "\n",
    "def ex_city(text):\n",
    "    if pd.isna(text):\n",
    "        return np.nan\n",
    "    city = text.split(',')[0].strip()\n",
    "    \n",
    "    if city == 'Москва':\n",
    "        return 'Москва'\n",
    "    elif city == 'Санкт-Петербург':\n",
    "        return 'Санкт-Петербург'\n",
    "    elif city in million_cities:\n",
    "        return 'город-миллионник'\n",
    "    else:\n",
    "        return 'другие'\n",
    "#извлекат готовность\n",
    "def extract_relocation(text):\n",
    "    if pd.isna(text):\n",
    "        return False\n",
    "    \n",
    "    text_lower = text.lower()\n",
    "    ready_keywords = ['готов', 'готова', 'готовы', 'хочу переехать']\n",
    "    not_ready_keywords = ['не готов', 'не готова', 'не готовы']\n",
    "    \n",
    "    for keyword in not_ready_keywords:\n",
    "        if keyword in text_lower:\n",
    "            return False\n",
    "    \n",
    "    for keyword in ready_keywords:\n",
    "        if keyword in text_lower:\n",
    "            return True\n",
    "    \n",
    "    return False\n",
    "\n",
    "def ex_trips(text):\n",
    "    if pd.isna(text):\n",
    "        #по умолчанию считаем не готовыми к командировкам\n",
    "        return False\n",
    "    \n",
    "    text_lower = text.lower()\n",
    "    parts = text.split(',')\n",
    "    business_trip_part = ' '.join(parts[-2:]).lower() if len(parts) >= 2 else text_lower\n",
    "    \n",
    "    if 'не готов' in business_trip_part and 'командировк' in business_trip_part:\n",
    "        return False\n",
    "    elif 'командировк' in business_trip_part:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "hh['Город'] = hh['Город, переезд, командировки'].apply(ex_city)\n",
    "hh['Готовность к переезду'] = hh['Город, переезд, командировки'].apply(extract_relocation)\n",
    "hh['Готовность к командировкам'] = hh['Город, переезд, командировки'].apply(ex_trips)\n",
    "print(hh['Город'].value_counts())\n",
    "print(hh['Готовность к переезду'].value_counts())\n",
    "print(hh['Готовность к командировкам'].value_counts())\n",
    "sample = hh[['Город, переезд, командировки', 'Город', 'Готовность к переезду', 'Готовность к командировкам']].head(15)\n",
    "print(sample.to_string())\n",
    "hh = hh.drop('Город, переезд, командировки', axis=1)\n",
    "print(hh.columns.tolist())"
   ]
