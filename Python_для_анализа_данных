    "\n",
    "log_age = np.log(hh['Age'])\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(12, 6))\n",
    "ax.hist(log_age, bins=50, color='steelblue', edgecolor='black', alpha=0.7)\n",
    "\n",
    "mean_log_age = log_age.mean()\n",
    "std_log_age = log_age.std()\n",
    "\n",
    "ax.axvline(mean_log_age, color='green', lw=2, label=f'Mean: {mean_log_age:.2f}')\n",
    "ax.axvline(mean_log_age - 3*std_log_age, color='red', lw=2, linestyle='--', label=f'Left 3σ: {mean_log_age - 3*std_log_age:.2f}')\n",
    "ax.axvline(mean_log_age + 4*std_log_age, color='red', lw=2, linestyle='--', label=f'Right 4σ: {mean_log_age + 4*std_log_age:.2f}')\n",
    "\n",
    "ax.set_xlabel('log(Age)')\n",
    "ax.set_ylabel('Count')\n",
    "ax.legend()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "z_scores = np.abs(stats.zscore(log_age))\n",
    "outliers = hh[z_scores > 4]\n",
    "\n",
    "print(f\"Найдено выбросов: {len(outliers)}\")\n",
    "if len(outliers) > 0:\n",
    "    print(outliers[['Age', 'ЗП (руб)', 'Опыт работы (месяц)']].head(10))\n",
    "    print(f\"Возраст выбросов: {outliers['Age'].min()} - {outliers['Age'].max()} лет\")\n",
    "\n",
    "# Удаление выбросов\n",
    "hh = hh[z_scores <= 4]\n",
    "print(f\"Строк после удаления выбросов возраста: {len(hh)}\")\n"
   ]
