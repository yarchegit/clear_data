   "source": [
    "sns.set_style(\"whitegrid\")\n",
    "fig, axes = plt.subplots(2, 4, figsize=(20, 12))\n",
    "\n",
    "# 1.\n",
    "age = hh['Age'].dropna()\n",
    "bins_age = np.arange(15, 76, 3)\n",
    "\n",
    "axes[0, 0].hist(age, bins=bins_age, color='steelblue', edgecolor='black')\n",
    "axes[0, 0].set_xlim(15, 70)\n",
    "axes[0, 0].set_xticks(range(15, 71, 5))\n",
    "axes[0, 0].set_xlabel('Возраст, лет')\n",
    "axes[0, 0].set_ylabel('Количество резюме')\n",
    "axes[0, 0].set_title('Распределение возраста')\n",
    "\n",
    "# 2. \n",
    "salary = hh['ЗП (руб)'].dropna()\n",
    "q_low, q_high = salary.quantile([0.01, 0.99])   # убираем крайние 1%\n",
    "salary_clipped = salary[(salary >= q_low) & (salary <= q_high)]\n",
    "\n",
    "axes[0, 1].hist(salary_clipped, bins=40, color='coral', edgecolor='black')\n",
    "axes[0, 1].set_xlabel('Зарплата, руб')\n",
    "axes[0, 1].set_ylabel('Количество резюме')\n",
    "axes[0, 1].set_title('Распределение зарплат (1–99 перцентили)')\n",
    "axes[0, 1].ticklabel_format(style='plain', axis='x')  # без 1e6\n",
    "\n",
    "# 3. \n",
    "valid = hh.dropna(subset=['Age', 'ЗП (руб)']).copy()\n",
    "sal = valid['ЗП (руб)']\n",
    "q_low, q_high = sal.quantile([0.01, 0.99])\n",
    "mask = (sal >= q_low) & (sal <= q_high)\n",
    "valid = valid[mask]\n",
    "\n",
    "axes[0, 2].scatter(valid['Age'], valid['ЗП (руб)'],\n",
    "                   alpha=0.3, s=10, color='green')\n",
    "axes[0, 2].set_xlim(15, 70)\n",
    "axes[0, 2].set_xticks(range(15, 71, 5))\n",
    "axes[0, 2].set_xlabel('Возраст, лет')\n",
    "axes[0, 2].set_ylabel('Зарплата, руб')\n",
    "axes[0, 2].set_title('Зарплата vs возраст (без крайних 1%)')\n",
    "axes[0, 2].ticklabel_format(style='plain', axis='y')\n",
    "\n",
    "# 4.\n",
    "education_salary = (hh.groupby('Образование')['ЗП (руб)']\n",
    "                      .median()\n",
    "                      .sort_values())\n",
    "education_salary.plot(kind='barh', ax=axes[0, 3],\n",
    "                      color='orange', edgecolor='black')\n",
    "axes[0, 3].set_xlabel('Медианная зарплата, руб')\n",
    "axes[0, 3].set_ylabel('Уровень образования')\n",
    "axes[0, 3].set_title('Медианная ЗП по образованию')\n",
    "axes[0, 3].ticklabel_format(style='plain', axis='x')\n",
    "\n",
    "# 5.\n",
    "relocation_salary = hh.groupby('Готовность к переезду')['ЗП (руб)'].mean()\n",
    "relocation_salary.plot(kind='bar', ax=axes[1, 0],\n",
    "                       color='purple', edgecolor='black')\n",
    "axes[1, 0].set_xlabel('Готовность к переезду')\n",
    "axes[1, 0].set_ylabel('Средняя зарплата, руб')\n",
    "axes[1, 0].set_title('ЗП vs готовность к переезду')\n",
    "axes[1, 0].ticklabel_format(style='plain', axis='y')\n",
    "\n",
    "# 6.\n",
    "business_salary = hh.groupby('Готовность к командировкам')['ЗП (руб)'].mean()\n",
    "business_salary.plot(kind='bar', ax=axes[1, 1],\n",
    "                     color='teal', edgecolor='black')\n",
    "axes[1, 1].set_xlabel('Готовность к командировкам')\n",
    "axes[1, 1].set_ylabel('Средняя зарплата, руб')\n",
    "axes[1, 1].set_title('ЗП vs командировки')\n",
    "axes[1, 1].ticklabel_format(style='plain', axis='y')\n",
    "\n",
    "# 7.\n",
    "employment_cols = [c for c in hh.columns if c.startswith('Занятость_')]\n",
    "employment_counts = {c.replace('Занятость_', ''): hh[c].sum()\n",
    "                     for c in employment_cols}\n",
    "pd.Series(employment_counts).sort_values().plot(\n",
    "    kind='barh', ax=axes[1, 2],\n",
    "    color='brown', edgecolor='black'\n",
    ")\n",
    "axes[1, 2].set_xlabel('Количество резюме')\n",
    "axes[1, 2].set_ylabel('Тип занятости')\n",
    "axes[1, 2].set_title('Распределение типов занятости')\n",
    "\n",
    "# 8.\n",
    "schedule_cols = [c for c in hh.columns if c.startswith('График_')]\n",
    "schedule_counts = {c.replace('График_', ''): hh[c].sum()\n",
    "                   for c in schedule_cols}\n",
    "pd.Series(schedule_counts).sort_values().plot(\n",
    "    kind='barh', ax=axes[1, 3],\n",
    "    color='darkred', edgecolor='black'\n",
    ")\n",
    "axes[1, 3].set_xlabel('Количество резюме')\n",
    "axes[1, 3].set_ylabel('График работы')\n",
    "axes[1, 3].set_title('Распределение графиков работы')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
