    "def new_col(text):\n",
    "    \n",
    "    words = text.split(' , ')[:2]\n",
    "    s_text = words[0]\n",
    "    a0_text = words[1].split(' ')\n",
    "    a_text = a0_text[1]\n",
    "    \n",
    "    if s_text == 'Мужчина':\n",
    "        return f'М, {a_text}'\n",
    "    else:\n",
    "        return f'Ж, {a_text}'\n",
